{
  "name": "@gchumillas/image-editor",
  "author": "Gonzalo Chumillas",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "build": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
